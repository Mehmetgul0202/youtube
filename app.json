{
  "name": "MusicBot",
  "description": "Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Mehmetgul0202/sonsurum1",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "ASSISTANT_NAME": {
      "description": "yardımcı kullanıcı adı ile doldurma @",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "your bot username from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": "Miss Music Bot"
    },
    "API_ID": {
      "description": "App ID my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash my.telegram.org/apps",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku api anahtarınız (Herokunun ayarlar menüsüne basarak alın) https://dashboard.heroku.com/account",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku uygulama isminiz. (Deploy etmeniz için vermiş olduğunuz name)",
      "required": false
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "60"
    }
  }
}
